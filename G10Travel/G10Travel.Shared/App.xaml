<Application
    x:Class="G10Travel.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:G10Travel">
    <Application.Resources>
        <LinearGradientBrush x:Key="background" EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF010837" Offset="0"/>
            <GradientStop Color="#FF0519A9" Offset="1"/>
        </LinearGradientBrush>
        <SolidColorBrush x:Key="selectionHighlight" Color="#FF0093FF"/>
    </Application.Resources>
</Application>
